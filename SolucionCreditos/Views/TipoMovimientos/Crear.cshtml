@using SolucionCreditos.Entities

@model TipoMovimiento


<form method="post">
    <div>
        <div class="form-group">
            <label>Nombre</label>
            <input class="form-control" asp-for="Concepto" required/>
        </div>

        <div class="form-group">
            <label>Entrada de capital</label>
            <input type="checkbox" asp-for="Deposito" />
        </div>

        <button type="submit" class="btn btn-primary">Guardar</button>
    </div>
</form>